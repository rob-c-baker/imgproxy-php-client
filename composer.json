{
    "name": "alanrogers/imgproxy-php-client",
    "description": "A PHP client for generating imgproxy URLs, includes Twig Extension for Twig projects (like Craft CMS).",
    "type": "library",
    "autoload": {
        "psr-4": {
            "ImgproxyPhpClient\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Rob Baker",
            "email": "rob.baker@alanrogers.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "ext-imagick": "*",
        "onliner/imgproxy-php": "0.2",
        "twig/twig": "~3.12.0"
    },
    "require-dev": {
        "craftcms/cms": "^5.3",
        "codeception/codeception": "^5.1",
        "codeception/module-asserts": "*",
        "codeception/verify": "^3.2",
        "codeception/assert-throws": "^1.4"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": false,
            "craftcms/plugin-installer": false
        }
    }
}
